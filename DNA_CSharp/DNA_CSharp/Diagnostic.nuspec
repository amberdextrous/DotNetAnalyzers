<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>DNA_CSharp</id>
    <version>1.0.0.0</version>
    <title>DNA .NET Analyzers (C#)</title>
    <authors>Alessandro Del Sole</authors>
    <owners>Alessandro Del Sole</owners>
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>https://github.com/AlessandroDelSole/DotNetAnalyzers</projectUrl>
    <iconUrl>http://www.visual-basic.it/Portals/0/Contents/images/dna_icon.png</iconUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>DNA .NET Analyzers, a collection of diagnostics and refactorings for C#</description>
    <releaseNotes>Summary of changes made in this release of the package.</releaseNotes>
    <copyright>Copyright (C), 2015 - Alessandro Del Sole</copyright>
    <tags>roslyn, analyzers</tags>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\dotnet and language specific analyzers in either analyzers\dotnet\cs or analyzers\dotnet\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>